{
  "name": "athena-ui",
  "version": "0.0.1",
  "license": "UNLICENSED",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config proxy.conf.json",
    "build:maven": "node ./node_modules/.bin/ng build --prod --deploy-url assets/ && npm run build:move",
    "build:local:prod": "ng build --prod --deploy-url assets/ && npm run build:move",
    "build:local:dev": "ng build --deploy-url assets/ && npm run build:move",
    "build:move": "cp -a dist/. ../priv/www/assets && mv ../priv/www/assets/index.html ../priv/www",
    "test": "ng test --source-map=false",
    "lint": "ng lint athena-ui",
    "lint-fix": "ng lint athena-ui --fix",
    "e2e": "ng e2e --port 49152 --proxy-config e2e-proxy.conf.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.0.6",
    "@angular/common": "^6.0.6",
    "@angular/compiler": "^6.0.6",
    "@angular/core": "^6.0.6",
    "@angular/forms": "^6.0.6",
    "@angular/http": "^6.0.6",
    "@angular/platform-browser": "^6.0.6",
    "@angular/platform-browser-dynamic": "^6.0.6",
    "@angular/router": "^6.0.6",
    "@clr/angular": "^0.12.1",
    "@clr/icons": "^0.12.1",
    "@clr/ui": "^0.12.1",
    "@ngx-translate/core": "~10.0.2",
    "@ngx-translate/http-loader": "^3.0.1",
    "@webcomponents/custom-elements": "^1.0.0",
    "bootstrap": "4.0.0-alpha.5",
    "core-js": "^2.4.1",
    "geojson-vt": "^3.1.3",
    "ngx-popper": "^4.1.0",
    "ngx-tour-core": "^3.0.0",
    "ngx-tour-ngx-popper": "^3.0.1",
    "ol": "^5.0.0",
    "popper.js": "^1.14.3",
    "rxjs": "^6.2.1",
    "vivus": "^0.4.4",
    "web3-eth-abi": "^1.0.0-beta.34",
    "zone.js": "^0.8.26"
  },
